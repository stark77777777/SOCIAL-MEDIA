      "cell_type": "code",
      "source": [
        "varlist =  ['ROLE PLAY']\n",
        "def binary_map(x):\n",
        "    return x.map({'YES': 1, 'NO': 0})\n",
        "data[varlist] = data[varlist].apply(binary_map)\n",
        "\n",
        "varlist1 =  ['MULTI PLAYER']\n",
        "def binary_map(x):\n",
        "    return x.map({'YES': 1, 'NO': 0})\n",
